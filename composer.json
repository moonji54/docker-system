{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "lenient": {
            "type": "composer",
            "url": "https://packages.drupal.org/lenient"
        },
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "1": {
            "type": "package",
            "package": {
                "name": "desandro/masonry",
                "version": "3.3.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/masonry/archive/v3.3.1.zip",
                    "type": "zip"
                }
            }
        },
        "2": {
            "type": "package",
            "package": {
                "name": "desandro/imagesloaded",
                "version": "3.1.8",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/imagesloaded/archive/v3.1.8.zip",
                    "type": "zip"
                }
            }
        },
        "3": {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "4.2.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        "4": {
            "type": "vcs",
            "url": "git@gitlab.com:sb-dev-team/soapbox-drupal-modules.git"
        },
        "5": {
            "type": "package",
            "package": {
                "name": "ckeditor/link",
                "version": "4.16.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/link/releases/link_4.16.0.zip",
                    "type": "zip"
                }
            }
        },
        "6": {
            "type": "package",
            "package": {
                "name": "ckeditor/fakeobjects",
                "version": "4.16.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.16.0.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "ckeditor/fakeobjects": "*",
        "ckeditor/link": "*",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.6.5",
        "desandro/imagesloaded": "3.1.8",
        "desandro/masonry": "3.3.1",
        "drupal/adminimal_admin_toolbar": "^1.8",
        "drupal/adminimal_theme": "^1.6",
        "drupal/allowed_formats": "^1.2",
        "drupal/anchor_link": "^2.5",
        "drupal/better_exposed_filters": "^5.0@beta|^5.0@RC|^5.0",
        "drupal/bugherd": "^1.0@beta",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/context": "^4.0@beta|^4.0@RC|^4.0",
        "drupal/context_active_trail": "^2.0@beta|^2.0@RC|^2.0",
        "drupal/cookiepro": "^2.0",
        "drupal/core": "^9.4",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/devel": "^4.0",
        "drupal/dropzonejs": "^2.3",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/field_group": "^3.1",
        "drupal/field_tools": "^1.0@alpha|^1.0@RC|^1.0",
        "drupal/file_delete": "^1.0",
        "drupal/focal_point": "^1.5",
        "drupal/form_mode_manager": "2.x-dev",
        "drupal/linked_field": "^1.3",
        "drupal/linkit": "^6.0@beta|^6.0@RC|^6.0",
        "drupal/login_redirect_per_role": "^1.2",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_remote_audio": "^1.0",
        "drupal/menu_admin_per_menu": "^1.3",
        "drupal/menu_block": "^1.5",
        "drupal/menu_breadcrumb": "^1.8",
        "drupal/metatag": "^1.8",
        "drupal/oembed_providers": "^2.0",
        "drupal/override_node_options": "^2.6",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/recaptcha": "^3.0",
        "drupal/recreate_block_content": "^2.1",
        "drupal/redirect": "^1.6.0",
        "drupal/role_delegation": "^1.0",
        "drupal/save_edit": "^1.1",
        "drupal/scheduled_transitions": "^2.3",
        "drupal/schema_metatag": "^2.3",
        "drupal/search_api": "^1.11",
        "drupal/simple_sitemap": "^3.0",
        "drupal/simple_user_management": "^1.0",
        "drupal/site_settings": "^1.13",
        "drupal/sitemap": "^2.0@beta|^2.0@RC|^2.0",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/svg_image": "^1.16",
        "drupal/symfony_mailer": "^1.1@beta|^1.0@RC|^1.0",
        "drupal/taxonomy_import": "^2.0",
        "drupal/token_default": "^1.0@beta|^1.0@RC|^1.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^2.1",
        "drupal/views_ajax_history": "^1.0",
        "drupal/viewsreference": "^2.0@beta|^2.0@RC|^2.0",
        "drupal/webform": "^6.0",
        "drupal/webp": "1.x-dev",
        "drush/drush": "^10",
        "enyo/dropzone": "4.2.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "sb-dev-team/drupal_modules": "dev-master",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^9",
        "kint-php/kint": "^4.1",
        "phpro/grumphp": "^1.13"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp-shim": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches": {
            "drupal/core": {
                "Error during custom module uninstallation : Call to a member function getConfigDependencyKey() on null": "https://www.drupal.org/files/issues/2019-07-17/custom-taxonomy-uninstallation.patch",
                "Entity References as checkboxes or dropdowns": "https://www.drupal.org/files/issues/2021-12-02/2429699-453-9.3.x.patch",
                "Supports OEmbed providers without thumbnails dimensions": "https://www.drupal.org/files/issues/2020-02-14/supports-oembed-providers-without-thumbnails-dimensions-3111991-5.patch",
                "PoStreamReader::readLine() throws an error on module install": "https://www.drupal.org/files/issues/2023-01-17/drupal_core-PoStreamReader_readLine_throws_an_error_on_module_install-3301239-7.patch",
                "#2700667: Notice: Undefined index: #type in Drupal\\Core\\Form\\FormHelper::processStates()": "https://www.drupal.org/files/issues/2021-05-12/2700667-133b.patch"
            },
            "drupal/field_group": {
                "Ensure visibility of invalid fields (JS)": "https://www.drupal.org/files/issues/2020-05-28/2787179-highlight-html5-validation-53.patch",
                "Undefined index name in form display": "https://www.drupal.org/files/issues/2020-08-10/field_group-undefined-index-name-3064890-16.patch"
            },
            "drupal/menu_admin_per_menu": {
                "Menu edit form is not visible on the node edit page": "https://www.drupal.org/files/issues/2018-11-30/2900957-default-menu-items.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
            "soapbox-drupal-modules"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/modules/soapbox": [
                "type:soapbox-drupal-modules"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/example.settings.local.php": false
            }
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
