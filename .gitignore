# Ignore directories generated by Composer
/drush/contrib/
/web/core/
/web/modules/contrib/
/web/modules/soapbox/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/.ddev/

# Don't commit the /vendor/ folder if you the host you're using doesn't need you to.
# Pantheon = You do need to commit the /vendor/ directory
# If the host is unknown, don't commit it until you know you need to.
/vendor/

# Ignore sensitive information
/web/sites/*/settings.local.php
/web/sites/*/settings.ddev.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env

# Ignore Soapbox theme files.
/web/themes/custom/*/assets/css/
/web/themes/custom/*/assets/img/symbol/svg/
/web/themes/custom/*/assets/src/node_modules/
